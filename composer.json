  "keywords": [
    "framework",
    "sample app",
    "php vue"
  ],
  "homepage": "https://projectsgroups.site",
  "license": "MIT",
  "authors": [
    {
      "name": "Ratana Chhorm",
      "email": "chhormratana@gmail.com"
    }
