    "ext-openssl": "*",
    "robmorgan/phinx": "^0.11.1",
    "swiftmailer/swiftmailer": "^6.2",
    "vlucas/phpdotenv": "^3.6",
    "php-di/php-di": "^6.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.5",
